<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/light_gray"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/mytopbar_whiteback"
        />
    <ScrollView
        android:scrollbars="none"
        android:fadingEdge="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="12dp">
                <include layout="@layout/line_gray" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:background="@android:color/white"
                android:paddingLeft="7dp"
                android:paddingRight="7dp"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/llHead"
                    android:background="@android:color/white"
                    android:paddingLeft="8dp"
                    android:paddingRight="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="64dp">
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.CB5_13"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="头像"
                        android:layout_weight="1" />
                    <com.example.sig.lianjiang.view.CircleImageView
                        android:id="@+id/imv_avatar"
                        android:layout_width="50dp"
                        android:layout_height="50dp"
                        android:src="@mipmap/icon_head" />
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="6dp"
                        android:src="@drawable/arrow_right1" />
                </LinearLayout>
                <include layout="@layout/line_gray" />
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/llNickname"
                    android:background="@android:color/white"
                    android:paddingLeft="8dp"
                    android:paddingRight="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="44dp">
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.CB5_13"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="昵称"
                        android:layout_weight="1" />
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.B0_13"
                        android:id="@+id/tvNickname"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="你的昵称"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/arrow_right1" />
                </LinearLayout>
                <include layout="@layout/line_gray" />
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/llMotto"
                    android:background="@android:color/white"
                    android:paddingLeft="8dp"
                    android:paddingRight="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="44dp">
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.CB5_13"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="个性签名"
                        android:layout_weight="1" />
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.B0_13"
                        android:id="@+id/tvMotto"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="明天世间怎去估计"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/arrow_right1" />
                </LinearLayout>
                <include layout="@layout/line_gray" />
                <LinearLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:id="@+id/service_tag"
                    android:background="@android:color/white"
                    android:paddingLeft="8dp"
                    android:paddingRight="2dp"
                    android:layout_width="match_parent"
                    android:layout_height="44dp">
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.CB5_13"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="我的标签"
                        android:layout_weight="1" />
                    <TextView
                        android:textAppearance="@style/TextAppear.Theme.B0_13"
                        android:gravity="right"
                        android:id="@+id/tips_nor"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:singleLine="true"
                        android:layout_weight="2"
                        android:text="暂未开发"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:src="@drawable/arrow_right1" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
