                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="密码修改"
                    android:layout_weight="1" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/arrow_right1" />
            </LinearLayout>
            <include layout="@layout/line_gray" />
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/change_phone"
                android:background="@android:color/white"
                android:paddingLeft="15dp"
                android:paddingRight="9dp"
                android:layout_width="match_parent"
                android:layout_height="44dp">
                <TextView
                    android:textAppearance="@style/TextAppear.Theme.CB5_13"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="修改手机号码"
                    android:layout_weight="1" />
                <TextView
                    android:textAppearance="@style/TextAppear.Theme.B0_13"
                    android:gravity="right"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:layout_weight="2"
                    android:text="12345678901"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:src="@drawable/arrow_right1" />
            </LinearLayout>
            <include layout="@layout/line_gray" />
            <LinearLayout
